<!--pages/compressionRatio/compressionRatio.wxml-->
<view class="page">
  <view class="page__hd">
    <view class="page__desc">输入O型圈线径和沟槽深度，将自动计算压缩率。</view>
  </view>
  <!--输入数据-->
  <form bindsubmit="formSubmit">
  <view class="page__bd3" >
   <view class="weui-cells__title">数据输入</view>
    <view class="weui-cells weui-cells_after-title">
        <!--O型圈线径-->
        <view class="weui-cell weui-cell_input weui-cell_vcode">
          <view class="weui-cell__hd">
            <view class="weui-label">O型圈线径(mm)</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" value="{{wireDiameter}}" name="inputWireDiameter" bindinput="bindInputWireDiameter" type="digit" placeholder="请输入O型圈线径"></input>
          </view>
        </view>
       <!--沟槽深度-->
        <view class="weui-cell weui-cell_input weui-cell_vcode">
          <view class="weui-cell__hd">
            <view class="weui-label">沟槽深度(mm)</view>
          </view>
          <view class="weui-cell__bd">
            <input class="weui-input" value="{{trenchDepth}}" name="inputTrenchDepth" bindinput="bindInputTrenchDepth" type="digit" placeholder="请输入沟槽深度"></input>
          </view>
        </view>
    </view>
  </view>
  </form>

  <!--输出数据-->
   <view class="page__bd">
    <view class="weui-cells__title">数据输出</view>
    <view class="weui-cells weui-cells_after-title">
         <view class="weui-cell weui-cell_input weui-cell_vcode">
        <view class="weui-cell__hd">
          <view class="weui-label">压缩率</view>
        </view>
        <view class="weui-cell__bd">
          <text class="weui-input" selectable="true">{{compressRatioOutput}}</text>
        </view>
        </view>
    </view>
   </view>
</view>

